{
  "name": "tween-one",
  "version": "1.2.2",
  "scripts": {
    "start": "dumi dev",
    "docs:deploy": "gh-pages -d dist",
    "build:npm": "father-build",
    "build": "dumi build",
    "deploy": "npm run docs && npm run docs:deploy",
    "release": "npm run build:npm && npm publish",
    "prepublishOnly": "npm run build:npm && np --no-cleanup --yolo --no-publish --any-branch",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "./lib/index",
  "module": "./es/index",
  "typings": "./es/index.d.ts",
  "sideEffects": false,
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "files": [
    "es",
    "lib",
    "assets/*.css",
    "assets/*.less"
  ],
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.11.1",
    "flubber": "^0.4.2",
    "raf": "^3.4.1",
    "style-utils": "^0.3.6",
    "svg-path-properties": "^1.0.4",
    "tween-functions": "^1.2.0"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "dumi": "^1.0.9",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "gsap": "^3.5.1",
    "lint-staged": "^10.0.7",
    "np": "^7.0.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "yorkie": "^2.0.0"
  }
}
